<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.g50.chitr"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="22" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.g50.chitr.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.g50.chitr.ActivityLanding"
            android:label="@string/title_activity_activity_landing" >
        </activity>
        <activity
            android:name="com.example.g50.chitr.ActivityFilters"
            android:label="@string/title_activity_activity_filters" >
        </activity>
        <activity
            android:name="com.example.g50.chitr.ActivityExport"
            android:label="@string/title_activity_activity_export" >
        </activity>
        <activity
            android:name="com.example.g50.chitr.register"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name="com.example.g50.chitr.Registration"
            android:label="@string/title_activity_registration" >
        </activity>
    </application>

</manifest>